            <ul id="uiv2-autosearch-list-alt" class="uiv2-top-searches" style="width:160px;margin:0;"><li><a href="{{this}}">{{@@key}}</a></li></ul>
        {{/each}}
        </div></script>
        <script id="tmplt-cart-items" type="text/x-handlebars-template">
        {{#each items}}
            <li><div class="uiv2-items-img"><a href="#"><img src="{{this.simg}}"  alt="{{this.description}}" width="65" height="61" ></a></div><div class="uiv2-items-content-wrapper"><div class="uiv2-items-content"><span><a href="#"> {{this.brand}} </a></span><p><a href="#">{{this.description}}</a></p></div><div class="uiv2-items-button-block"><div class="uiv2-grid-count-btn"><button class="icon icon-decrease-qty-search-popup">-</button><input type="text" class="text-change-qty-search-popup" id="p_{{this.pid}}" value="{{this.quantity}}" maxlength="{{max_qty_length}}" /><button class="icon icon-increase-qty-search-popup">+</button></div><span class="uiv2-items-rate"><span class="WebRupee">Rs.</span> {{this.mrp}}</span>
