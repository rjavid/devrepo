        <script id="tmplt-autosearch" type="text/x-handlebars-template">
       {{#if data}}
        <ul id="uiv2-autosearch-list">
       {{/if}}
       {{#each data}}
            {{#each this.store_details_to_use}}
                {{#if ../this.name}}
                    <li>
                        {{#if ../../this.simg}}
                            {{#if ../../../alt_suggestions}}
                                <a href="{{../../this.url}}?nc=sugg-as" class="uiv2-img-product-search"><img src="{{../../this.simg}}" alt="" height="30" width="30"></a>
                            {{else}}
                                <a href="{{../../this.url}}?nc=as" class="uiv2-img-product-search"><img src="{{../../this.simg}}" alt="" height="30" width="30"></a>
                            {{/if}}
                        {{else}}
                            <div class="uiv2-push-left" style="width:30px; height:30px;"></div>
                        {{/if}}
                        {{#if ../../alt_suggestions}}
                            <a href="{{../../../this.url}}?nc=sugg-as" class="uiv2-img-product-name"><p>{{../../../this.brand}}</p><span>{{../../../this.name}}</span></a>
                        {{else}}
                            <a href="{{../../../this.url}}?nc=as" class="uiv2-img-product-name"><p>{{../../../this.brand}}</p><span>{{../../../this.name}}</span></a>

                        {{/if}}
                        <div class="uiv2-product-weight"><span>{{../../this.weight}}</span></div><div class="uiv2-product-cost"><span><span class="WebRupee">Rs.</span> {{../../this.price}}</span></div><!----START for standard and express icon------------------>
                            {{#ifCond this.fi_type "darkstore-express"}}
                                <div class="autoSearch_express"><a href="" class="exp-slot autoSearch_express" title="{{this.next_slot}}"></a><input type="hidden" name='cart-item-mode'
                                           id="cartitem_mode_{{../../this.pid}}"
                                           value="express" /></div>
                            {{else}}
                                {{#ifCond this.fi_type "jit"}}
                                    <div class="autoSearch_standard"><a href="" class="normal-slot autoSearch_standard" title="{{this.next_slot}}"></a><input type="hidden" name='cart-item-mode'
                                               id="cartitem_mode_{{../../this.pid}}"
                                               value="normal" /></div>
                                {{else}}
                                    <div class="autoSearch_standard"><a href="" class="normal-slot autoSearch_standard" title="{{this.next_slot}}"></a><input type="hidden" name='cart-item-mode'
                                               id="cartitem_mode_{{../../this.pid}}"
                                               value="normal" /></div>
                                {{/ifCond}}
                            {{/ifCond}}
                        <!----END for standard and express icon------------------><div class="uiv2-rate-count-btn uiv2-search-qty-widget"><span class="uiv2-qty-label">Qty</span><input value="1" type="text" id="uiv2-qty-{{../../this.pid}}" name="" maxlength="{{../../max_qty_length}}"></div><div class="uiv2-product-add-btn">

                            {{#ifCond ../../this.availability  2}}
                                <img src="/static/../../images/searchOutOfStock.png" alt="Out of stock"
                                     style="width:60px; margin-top: 10px;" />
                            {{else}}
                                <input class="uiv2-a2c-autosearch" value="ADD" type="button"
                                       id="uiv2-a2c-{{../../../this.pid}}">
                            {{/ifCond}}

                        </div><input type="hidden" id="uiv2-pdesc-{{../../this.pid}}" value="{{../../this.text}}" /></li>
                {{/if}}
            {{/each}}
       {{else}}
            {{#unless alt_suggestions}}
                <p class="uiv2-commonly-searched" style="margin-left: 8px;">Commonly Searched</p>
                {{#each top_searches}}
                <ul id="uiv2-autosearch-list" class="uiv2-top-searches">
                {{#each this}}
                    <li><a href="/ps/?q={{this}}">{{{ this}}}</a></li>
                {{/each}}
                </ul>
                {{/each}}
            {{/unless}}
       {{/each}}
